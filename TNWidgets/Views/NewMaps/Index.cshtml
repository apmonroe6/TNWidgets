<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no" />
    <style type="text/css">
      html { height: 100% }
      body { height: 400px; width: 400px; margin: 0; padding: 0 }
      #map_canvas { height: 400px; width: 400px; }
    </style>
    <script type="text/javascript"
      src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDxTFgdL1vUeJQVAEcxhzRgosbW8F8k8sY&sensor=false">
    </script>
    <script type="text/javascript">
        function initialize() {
            var stylesArray = [
                {
                    featureType: "poi",
                    elementType: "all",
                    stylers: [
                        { visibility: 'off' }
                    ]
                },
                {
                    featureType: "transit",
                    elementType: "all",
                    stylers: [
                        { visibility: 'off' }
                    ]
                },
                {
                    featureType: "road",
                    elementType: "all",
                    stylers: [
                        { visibility: 'off' }
                    ]
                },
                {
                    featureType: "landscape",
                    elementType: "geometry",
                    stylers: [
                        { hue: "#F48021" },
                        { saturation: 100 },
                        { lightness: -30 }
                    ]
                },
                {
                    featureType: "administrative",
                    elementType: "geometry",
                    stylers: [
                        { hue: "#F48021" },
                        { saturation: 100 },
                        { lightness: -30 }
                    ]
                },
                {
                    featureType: "water",
                    elementType: "geometry",
                    stylers: [
                        { hue: "#2A2A72" },
                        { saturation: 100 },
                        { lightness: 0 }
                    ]
                },
                {
                    featureType: "all",
                    elementType: "labels",
                    stylers: [
                        { visibility: 'off' }
                    ]
                }
            ]
            var mapOptions = {
                center: new google.maps.LatLng(37.397, -85.5),
                zoom: 5,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                styles: stylesArray,
                disableDefaultUI: true

            };
            var map = new google.maps.Map(document.getElementById("map_canvas"),
            mapOptions);

            var atlLatLng = new google.maps.LatLng(33.7489, -84.3881)
            var atlMarker = new google.maps.Marker({
                position: atlLatLng,
                map: map
            });
            var chiLatLng = new google.maps.LatLng(41.8500, -87.6500)
            var chiMarker = new google.maps.Marker({
                position: chiLatLng,
                map: map
            });

        }
    </script>
</head>
<body onload="initialize()">
    <div id="map_canvas" style="width:100%; height:100%"></div>
</body>
</html>
